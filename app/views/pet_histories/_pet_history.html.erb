<div id="<%= dom_id pet_history %>">
  
  <p>
  <strong>Fecha:</strong>
    <%= pethistory.created_at %>
  </p>
  
  <p>
    <strong>Dueño:</strong>
    <%= pethistory.owner %>
  </p>
  
  <p>
    <strong>Mascota:</strong>
    <%= pethistory.pet_name %>
  </p>
  <p>
    <strong>Peso:</strong>
    <%= pet_history.weight %>
  </p>

  <p>
    <strong>Altura:</strong>
    <%= pet_history.height %>
  </p>

  <p>
    <strong>Descripción:</strong>
    <%= pet_history.description %>
  </p>

</div>
